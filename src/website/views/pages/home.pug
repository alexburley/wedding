style.
  .home-itinerary {
    max-width: 60%;
    margin: 30px auto;
  }

  .home-itinerary-address p {
    font-weight: 500;
    margin: 0;
  }

  .home-itinerary .home-address-title {
    font-size: 42px;
    letter-spacing: 0.03em;
    line-height: 110%;
    padding-bottom: 10px;
  }

  .home-itinerary-wrapper {
    display: flex;
    flex-direction: column;
    max-width: 300px;
    margin: auto;
  }

  .home-itinerary-slot {
    display: flex;
    text-align: left;
    flex-grow: 1;
    gap: 50px;
    margin: 10px 0;
    line-height: 125%;
  }
.container
